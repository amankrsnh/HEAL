        <servlet-name>ShowAllDoctorsServlet</servlet-name>
        <url-pattern>/ShowAllDoctorsServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
