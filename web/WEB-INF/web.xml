    </servlet>
    <servlet>
        <servlet-name>ShowPatientsControllerServlet</servlet-name>
        <servlet-class>heal.controller.ShowPatientsControllerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetPatientControllerServlet</servlet-name>
        <servlet-class>heal.controller.GetPatientControllerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdatePatientControllerSevlet</servlet-name>
        <servlet-class>heal.controller.UpdatePatientControllerSevlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowAllDoctorsServlet</servlet-name>
        <servlet-class>heal.controller.ShowAllDoctorsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistrationControllerServlet</servlet-name>
        <url-pattern>/RegistrationControllerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginControllerServlet</servlet-name>
        <url-pattern>/LoginControllerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowDoctorsControllerServlet</servlet-name>
        <url-pattern>/ShowDoctorsControllerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DoctorControllerServlet</servlet-name>
        <url-pattern>/DoctorControllerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PatientControllerServlet</servlet-name>
        <url-pattern>/PatientControllerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminControllerServlet</servlet-name>
        <url-pattern>/AdminControllerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetDoctorControllerServlet</servlet-name>
        <url-pattern>/GetDoctorControllerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateDoctorControllerSevlet</servlet-name>
